<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.zerock.wego.mapper.BadgeMapper">

 	<select id="selectAllSan"
		resultType="org.zerock.wego.domain.badge.BadgeVO">

		SELECT 
			*
		FROM 
			badge_tb
		WHERE
			badge_gb = '산'
			
	</select>
	
	
	<select id="selectAllRanking"
		resultType="org.zerock.wego.domain.badge.BadgeVO">

		SELECT 
			*
		FROM 
			badge_tb
		WHERE
			badge_gb = '랭킹'
			
	</select>
	
	
</mapper>
